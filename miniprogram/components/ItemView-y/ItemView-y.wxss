.swItem{
  width: 358rpx;background:#fff;border-radius: 5rpx;margin-bottom: 25rpx;
}
.pro_pic{
  width:30rpx;height: 30rpx;border-radius: 15rpx;
}
.swItem .pic{
  width: 100%;
}
.swItem .pic image{
  width: 100%;
}
.swItem .txt{
  display: flex;flex-direction: column;justify-content: space-between;
  padding: 0 8rpx 20rpx ;
}
.swItem .txt .des{
  font-size: 22rpx;display: flex;justify-content: space-between;
  padding-top: 25rpx;
}
.swItem .txt .pic{
  width: 90rpx;
}
.swItem .txt .pic image{
  width: 22rpx;
}
.des .peo_info{
  display: flex;
}
.des .type{
  padding-left: 20rpx;
}
.swItem .txt .tit{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}